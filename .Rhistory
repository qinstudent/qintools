# 生成虚构的数据
set.seed(2024)
n <- 100 # 患者数量
group <- sample(c("A","B"),n,replace=TRUE) # 随机分组
entry <- runif(n,0,30) # 入组时间
pfs <- rexp(n,0.05) # PFS时间
os <- pfs + rexp(n,0.03) # OS时间
progression <- sample(c("PD","SD","PR","CR"),n,replace=TRUE) # 疾病进展的原因
death <- sample(c("Disease","Other","Censored"),n,replace=TRUE) # 死亡的原因
data <- data.frame(group,entry,pfs,os,progression,death)
View(data)
